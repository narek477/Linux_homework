#include <iostream>
#include <cstdlib>

int main(int charc, char* argv[]) 
{
	int a = std::atoi(argv[1]);
	int b = std::atoi(argv[2]);
	int c = std::atoi(argv[3]);
	
	int min = a;
	if(b < min){
		min = b;
	}

	if(c < min) {
		min = c;
	}

	int max = a;
	if(b > max) {
		max = b;
	}

	if(c > max) {
		max = c;
	}

	std::cout << "min-" << min << ", max-" << max << std::endl;

	return 0;
}
